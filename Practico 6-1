// Serie.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	int n,i,suma;
	cout<<"Ingrese la cantidad de numeros: ";
	cin>>n;
	suma=0;
	for (i=1;i<=n;i++)
		suma=suma+i;
		cout<<"\nEl resultado de la suma es: "<<suma;
	getch();
}
