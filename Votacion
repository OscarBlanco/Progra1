#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "stdio.h"
using namespace std;

void main() 
{
	int a, b;
	a = 18;
	cout << "Ingrese edad: ";
	cin >> b;
	if ((b>=a)) 
	{
		cout << "La persona si puede votar";
	}
	if ((b<a)) 
	{
		cout << "La persona no puede votar";
	}
	getch();
}
